{
  "name": "shindakioku/request-hydrator",
  "type": "library",
  "description": "Request hydrator into DTO with Validation",
  "license": "MIT",
  "homepage": "https://github.com/shindakioku/request-hydrator",
  "support": {
    "issues": "https://github.com/shindakioku/request-hydrator/issues",
    "source": "https://github.com/shindakioku/request-hydrator"
  },
  "keywords": [
    "php",
    "request",
    "dto",
    "validator"
  ],
  "authors": [
    {
      "name": "Anton M.",
      "email": "shindakioku@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "illuminate/http": "^5.8@dev",
    "illuminate/translation": "^5.8@dev",
    "illuminate/validation": "^5.8@dev",
    "league/container": "dev-master",
    "lstrojny/functional-php": "dev-master",
    "php-slang/php-slang": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "require-dev": {
    "phpunit/phpunit": "8"
  }
}
